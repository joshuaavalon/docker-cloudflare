{
  "name": "cloudflare-ddns",
  "version": "3.3.0",
  "description": "",
  "type": "module",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "prerollup": "rimraf dist",
    "rollup": "rollup -c",
    "start:pretty": "node dist/index.js | pino-pretty --translateTime yyyy-mm-dd'T'HH:MM:sso",
    "start:json": "node dist/index.js",
    "lint": "eslint src",
    "generate": "node script/json-schema.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joshuaavalon/docker-cloudflare.git"
  },
  "keywords": [
    "ddns",
    "cloudflare"
  ],
  "author": "Joshua Avalon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joshuaavalon/docker-cloudflare/issues"
  },
  "homepage": "https://github.com/joshuaavalon/docker-cloudflare#readme",
  "imports": {
    "#api": {
      "types": "./src/api/index.ts",
      "import": "./dist/api/index.js"
    },
    "#config": {
      "types": "./src/config/index.ts",
      "import": "./dist/config/index.js"
    },
    "#context": {
      "types": "./src/context/index.ts",
      "import": "./dist/context/index.js"
    },
    "#func": {
      "types": "./src/func/index.ts",
      "import": "./dist/func/index.js"
    },
    "#ip-echo": {
      "types": "./src/ip-echo/index.ts",
      "import": "./dist/ip-echo/index.js"
    },
    "#ip-echo/*": {
      "types": "./src/ip-echo/*",
      "import": "./dist/ip-echo/*"
    },
    "#error": {
      "types": "./src/error/index.ts",
      "import": "./dist/error/index.js"
    }
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./dist/index.js"
    }
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "25.0.5",
    "@rollup/plugin-json": "6.0.1",
    "@rollup/plugin-node-resolve": "15.2.3",
    "@rollup/plugin-typescript": "11.1.5",
    "@rollup/wasm-node": "4.5.0",
    "@types/lodash-es": "4.17.11",
    "@types/node": "20.9.2",
    "@typescript-eslint/eslint-plugin": "6.11.0",
    "@typescript-eslint/parser": "6.11.0",
    "eslint": "8.54.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.1",
    "globals": "13.23.0",
    "prettier": "3.1.0",
    "rimraf": "5.0.5",
    "typescript": "5.2.2"
  },
  "dependencies": {
    "@sinclair/typebox": "0.31.27",
    "ajv": "8.12.0",
    "cosmiconfig": "8.3.6",
    "cron": "3.1.6",
    "dotenv": "16.3.1",
    "lodash-es": "4.17.21",
    "pino": "8.16.0",
    "pino-pretty": "10.2.3",
    "read-env-vars": "1.1.1"
  }
}
